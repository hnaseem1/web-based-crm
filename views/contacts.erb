<!DOCTYPE html>
<html>
  <head>
    <meta charset="utf-8">
    <title>Contacts</title>
  </head>
  <body>
    <h1>Welcome to my Contacts Page</h1>
    <% @contacts.each do |contact| %>

      <ul>
        <li>
          <%= contact.full_name %>
          <li>
            <%= contact.email %>
          </li>
          <li>
            <%= contact.note %>
          </li>
        </li>
      </ul>

    <% end %>
  </body>
</html>
